<%- include("PageHeader") %>

<body>
  <!-- Page Preloder -->
  <div id="preloder">
    <div class="loader"></div>
  </div>

  <div class="wrap">
    <div style="height:100px" class="header">
      <div class="header-title">
        <div class="header-title-img">
          <img src="../img/logo.png" alt="" />
        </div>
        <div class="header-title-btn">
          <div class="header-title-btn__name">Hello, <%=CustomerName%></div>
          <a
            href="/logout"
            class="mybtn test__link"
            style="background-color: #f14e4e;"
          >
            <i class="fa fa-power-off" aria-hidden="true"></i>
            Sign Out
          </a>
        </div>
	  </div>
	  
      <nav style="visibility:collapse;" class="header-menu">
        <ul class="menu-lv1">
          <li>
            <i class="fas fa-university"></i> Transfer
            <ul class="menu-lv2">
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
            </ul>
          </li>
          <li>
            <i style="margin: 0 5px 0 0;" class="fa fa-credit-card-alt"></i>
            Account
            <ul class="menu-lv2">
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
            </ul>
          </li>
          <li>
            <i class="fa fa-history"></i> Transaction
            <ul class="menu-lv2">
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
            </ul>
          </li>
          <li>
            <i class="fa fa-info-circle"></i> Customer
            <ul class="menu-lv2">
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
            </ul>
          </li>
          <li>
            <i style="margin: 0 5px 0 0;" class="fa fa-money"></i> Saving
            <ul class="menu-lv2">
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
            </ul>
          </li>
          <li>
            <i class="fas fa-headphones"></i> Support
            <ul class="menu-lv2">
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
              <li>item 01</li>
            </ul>
          </li>
        </ul>
      </nav>
    </div>
    <div class="content gradient-bg">
      <section class="features-section gradient-bg">
        <div class="container text-white">
          <div class="section-title text-center content-title">
            <h2 style="font-size:4rem;">Baking Features</h2>
			<p>Free and convenient transactions.</p>
			 <%- include('ShowMessage') %> 
          </div>
          <div class="row content-section">
            <!-- feature -->
           <div class="col-md-6 col-lg-3 feature content-section-item">
					<i class="fas fa-university"></i>
					<div class="feature-content">
						<a href="/bank-transfer">
							<h4>Bank Transfer</h4>
						</a>
						<p>Transfer, receive money immediately. </p>
					</div>
				</div>
			<!-- feature -->
			<div class="col-md-6 col-lg-3 feature content-section-item">
					<i class="fa fa-credit-card-alt"></i>
					<div class="feature-content">
						<a href="/account">
							<h4>Account</h4>
						</a>
						<p>Saving or Payment Account </p>
					</div>
				</div>
				
				
				
				<div class="col-md-6 col-lg-3 feature content-section-item">
					<i class="fa fa-info-circle"></i>
					<div class="feature-content">
						<a href="/customer">
							<h4>Customer</h4>
						</a>
						<p>Information of Customer</p>
					</div>
				</div>
				
				<div class="col-md-6 col-lg-3 feature content-section-item">
					<i  class="fa fa-money"></i>
					<div class="feature-content">
						<a href="/bank-saving">
							<h4>Bank Saving</h4>
						</a>
						<p>High interest rates, Stable, Safe</p>
					</div>
				</div>
				
				<div class="col-md-6 col-lg-3 feature content-section-item">
					<i  class="fas fa-headphones"></i>
					<div class="feature-content">
						<a href="/test">
							<h4>Support - Help</h4>
						</a>
						<p>Friendly, enthusiastic. </p>
					</div>
				</div>
			
            </div>
      </section>
    </div>
  </div>
</body>


